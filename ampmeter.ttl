@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .

@prefix ampmeter: <https://johannes-mueller.org/lv2/ampmeter-rs#> .

ampmeter:ui
        a ui:X11UI ;
        lv2:requiredFeature ui:idleInterface; lv2:extensionData ui:idleInterface;
        lv2:requiredFeature urid:map ;
  .


ampmeter:lv2
    a lv2:Plugin ,
        lv2:AmplifierPlugin ;

    lv2:project <https://johannes-mueller.org/lv2/ampmeter-rs> ;
    doap:name "Simple Amplifier (Rust Version)" ;
    doap:license <http://opensource.org/licenses/isc> ;

    lv2:optionalFeature lv2:hardRTCapable ;

    ui:ui ampmeter:ui ;

    lv2:port [

        a lv2:InputPort ,
            lv2:ControlPort ;
            lv2:index 0 ;
        lv2:symbol "gain" ;
        lv2:name "Gain" ,
            "收益"@ch ,
            "Gewinn"@de ,
            "Gain"@en-gb ,
            "Aumento"@es ,
            "Gain"@fr ,
            "Guadagno"@it ,
            "利益"@jp ,
            "Увеличение"@ru ;

        lv2:default 0.0 ;
        lv2:minimum -90.0 ;
        lv2:maximum 24.0 ;
        units:unit units:db ;
        lv2:scalePoint [
            rdfs:label "+5" ;
            rdf:value 5.0
            ] , [
            rdfs:label "0" ;
            rdf:value 0.0
            ] , [
            rdfs:label "-5" ;
            rdf:value -5.0
            ] , [
            rdfs:label "-10" ;
            rdf:value -10.0
            ]
        ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:symbol "enabled" ;
        lv2:name "Enabled" ;

        lv2:default 1 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
        lv2:portProperty lv2:integer, lv2:toggled ;
        ] , [
        a lv2:OutputPort,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "input_level" ;
        lv2:name "Input Level" ;
        lv2:minimum -160.0 ;
	lv2:maximum 40.0;
	units:unit units:db
        ] , [
        a lv2:OutputPort,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "output_level" ;
        lv2:name "Output Level" ;
        lv2:minimum -160.0 ;
	lv2:maximum 40.0;
	units:unit units:db
        ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 4 ;
        lv2:symbol "in" ;
        lv2:name "In"
        ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 5 ;
        lv2:symbol "out" ;
        lv2:name "Out"
        ] .
